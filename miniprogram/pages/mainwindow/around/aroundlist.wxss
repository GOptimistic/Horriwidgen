page{
  background: #eeecec;
}

.over-search{
  height: 110rpx;
  width: 100%;
  background-color: whitesmoke;
  position: fixed;
  top: 0;
}

.search {
  margin: 0 auto;
  width: 95%;
  height: 80rpx;
  background-color: white;
  margin-top: 15rpx;
  border: 1rpx solid white;
  border-radius: 10rpx;
  margin-left: 2.5%;
}

.around-items{
  height: 150rpx;
  width: 100%;
  background-color: white;
  margin-top: 5rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}

.around-item{
  width: 95%;
  margin-left: 2.5%;
  background: transparent;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
}

.around-list{
  margin-top: 115rpx;
}

.person-line {
  width: 100%;
  height: 2rpx;
  background: gray;
  margin-top:13rpx; 
}

.h1{
  margin: 3px 0;
  font-size:35rpx;
}

.h5{
  font-size: 25rpx;
}

.h6{
  font-size: 20rpx;
  color: lightgray;
  margin-right: 10rpx;
  margin-top: 50rpx;
}

