/* client/pages/mainwindow/rooms/rooms.wxss */
.listy{
  padding: 30rpx;
}
.listy .item{
  width: 700rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  border:2px double #2BD5D5;
  border-radius: 20rpx;
  margin-bottom: 20rpx;
}
.listy .item .container-info{
  width: 700rpx;
  padding-left: 50rpx;
  display: flex;
  flex-direction: row;
  align-items: space-between;
  font-size: 30rpx;
}
.listy .item .container-info.roominfo{
  width: 500rpx;
  display: flex;
  flex-direction: column;
  align-items: space-between;
}
.listy .item .container-btn{
  width: 500rpx;
  height: 70rpx;
  padding-left: 200rpx;
  display: flex;
  flex-direction: row;
  align-items: space-between;
  font-size: 10rpx;
}



